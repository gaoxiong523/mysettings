<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Class...">
        <Item>
          <IdeaActionID>GotoClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+T'</ShortCut>
          <Description>Class...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Close All">
        <Item>
          <IdeaActionID>CloseAllEditors</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F4' or 'Ctrl+Shift+W'</ShortCut>
          <Description>Close All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F3' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Ctrl+Alt+L' or 'Ctrl+Shift+F'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Select In...">
        <Item>
          <IdeaActionID>SelectIn</IdeaActionID>
          <ShortCut>'Alt+F1'</ShortCut>
          <Description>Select In...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>